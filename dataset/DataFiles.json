{
	"name": "DataFiles",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_AzureBlobStorage_Dev",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"pFileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().pFileName",
					"type": "Expression"
				},
				"container": "rawfiles"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "Row",
				"type": "String"
			},
			{
				"name": "Country",
				"type": "String"
			},
			{
				"name": "Year",
				"type": "String"
			},
			{
				"name": "Product_Type",
				"type": "String"
			},
			{
				"name": "Sales",
				"type": "String"
			}
		]
	}
}